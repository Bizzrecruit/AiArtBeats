{
  "version": 2,
  "builds": [
      {
          "src": "server.js",
          "use": "@vercel/node"
      },
      {
          "src": "public/**",
          "use": "@vercel/static"
      }
  ],
  "routes":[
      {
          "src": "/api/(.*)",
          "dest": "server.js"
      },
      {
          "src": "/",
          "dest": "public/index.html"
      },
      {
        "src": "/",
        "dest": "public/gallery.js"
      },
      {
      "src": "/",
      "dest": "public/gallery.css"
      },
      {
          "src": "/(.+)",
          "dest": "public/$1"
      }
  ]
}